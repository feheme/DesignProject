@model EntityLayer.Concrete.Staff
@{
    ViewData["Title"] = "AddStaff";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<form method="post">
    <label>Personel Adı</label>
    <input type="text" asp-for="Name" class="form-control"/>
    <br />
     <label>Personel Ünvanı</label>
    <input type="text" asp-for="Title" class="form-control"/>
    <br />
     <label>Fotoğraf</label>
    <input type="text" asp-for="Photo" class="form-control"/>
    <br />
     <label>Sosyal Medya2</label>
    <input type="text" asp-for="SocialMedia2" class="form-control"/>
    <br />
     <label>Sosyal Medya3</label>
